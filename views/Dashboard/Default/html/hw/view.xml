<?xml version="1.0" encoding="UTF-8"?>
<view>
	<name>Dashboard Default HW</name>
	<displayname>Hello World Component View (Dashboard/Default)</displayname>
	<description>Dashboard Default View for HW</description>
	<version>0.0.1</version>
	<repo>https://github.com/h-w-f/view-dashboard-default-hw</repo>
	<dependencies>
		<application>
			<name>Dashboard</name>
			<version>0.0.1</version>
			<repo>https://github.com/h-w-f/application-dashboard</repo>
		</application>
		<component>
			<name>Hw</name>
			<version>0.0.1</version>
			<repo>https://github.com/h-w-f/component-dashboard-hw</repo>
		</component>
	</dependencies>
	<settings>
		<head>
			<title>Dashboard - HW</title>
			<meta>
				<name>viewport</name>
				<content>width=device-width, initial-scale=1, shrink-to-fit=no</content>
			</meta>
			<meta>
				<name>description</name>
				<content>Dashboard Views</content>
			</meta>
			<meta>
				<name>author</name>
				<content>Guru</content>
			</meta>
			<meta>
				<name>keywords</name>
				<content>HTML, CSS, JavaScript</content>
			</meta>
			<link>
				<remove>true</remove>
				<href>/views/Dashboard/Default/css/bootstrap.min.css</href>
			</link>
			<script>
				<src>/views/Dashboard/Default/js/jquery-3.5.1.min.js</src>
			</script>
			<script>
				<remove>true</remove>
				<src>/views/Dashboard/Default/js/bootstrap.min.js</src>
			</script>
		</head>
		<body>
			<params>id="body" class="custom-body"</params>
		</body>
		<footer>
			<params>class="custom-footer"</params>
			<script>
				<src></src>
			</script>
		</footer>
	</settings>
</view>